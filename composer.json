{
    "type": "project",
    "keywords": [
        "symfony",
        "vankosoft",
        "application"
    ],
    "homepage": "http://vankosoft.org",
    "license": "proprietary",
    "authors": [
        {
            "name": "Ivan I. Atanasov"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "ext-ctype": "*",
        "ext-iconv": "*",
        
        "symfony/dotenv": "5.4.*",
        "symfony/flex": "^1.17|^2",
        "symfony/notifier": "5.4.*",
        "symfony/runtime": "5.4.*",
        
        "vankosoft/application": "1.8.*",
        
        "vankosoft/users-subscriptions-bundle": "dev-develop",
        "vankosoft/payment-bundle": "dev-develop"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "5.4.*",
        "symfony/debug-bundle": "5.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.0",
        "symfony/stopwatch": "5.4.*",
        "symfony/var-dumper": "^5.3",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "symfony/flex": true,
            "oomphinc/composer-installers-extender": true,
            "symfony/runtime": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "assets:install public/admin-panel": "symfony-cmd",
            "cache:clear": "symfony-cmd",
            "liip:imagine:cache:remove": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
