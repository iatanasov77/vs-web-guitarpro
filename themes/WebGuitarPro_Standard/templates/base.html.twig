<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}.:: VS WebGuitarPro{% endblock %}</title>
        {% block stylesheets %}
        	{{ encore_entry_link_tags( 'app', null, 'WebGuitarPro_Standard' ) }}
        {% endblock %}
    </head>
    <body>
        <div class="wrapper">
            <!-- Page Content  -->
            <div id="content">
                {% include 'Layout/top.html.twig' %}
                
    		    {% block content %}{% endblock %}
    		</div>
    	</div>
    	
        {% block widgets %}{% endblock %}
        {% block svg %}{% endblock %}
        
        {% block javascripts %}
            {#
            <script src="https://code.jquery.com/jquery-2.2.4.min.js"></script>
            
            <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.11.0/umd/popper.min.js" integrity="sha384-b/U6ypiBEHpOf/4+1nzFpr53nxSS+GLCkfwBdFNTxtclqqenISfwAzpKaMNFNmj4" crossorigin="anonymous"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/javascript.util/0.12.12/javascript.util.min.js"></script>
            <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js"></script>
            <script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-slider/9.7.2/bootstrap-slider.min.js" integrity="sha256-dd5QxcG/E8fb/H/iIQann2y2EDS0nEMVI56yYWFeeW4=" crossorigin="anonymous"></script>
            #}
            
            
        	{{ encore_entry_script_tags( 'app', null, 'WebGuitarPro_Standard' ) }}

        <!--  
        	<script type="text/javascript" src="/assets/login.js"></script>
        -->
        {% endblock %}
    </body>
</html>
